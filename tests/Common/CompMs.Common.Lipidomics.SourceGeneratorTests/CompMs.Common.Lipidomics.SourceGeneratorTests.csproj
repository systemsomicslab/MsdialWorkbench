    </Content>
    <Content Include="LipidSpectrumGeneratorTest_GL.msp">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="LipidSpectrumGeneratorTest_GP.msp">
